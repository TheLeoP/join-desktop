{
  "name": "join-desktop",
  "version": "1.5.0",
  "description": "A modern Desktop Join application that works on Linux, MacOS and Linux, and doesn't requires Google Chrome to work.",
  "main": "./out/main/index.mjs",
  "author": "Luis Calle (https://github.com/TheLeoP)",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "publish:win": "npm run build && electron-builder --win --publish always",
    "publish:mac": "electron-vite build && electron-builder --mac --publish always",
    "publish:linux": "electron-vite build && electron-builder --linux --publish always"
  },
  "build": {
    "snap": {
      "publish": "github"
    },
    "publish": "github"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@fastify/auth": "^5.0.2",
    "@fastify/bearer-auth": "^10.1.1",
    "@fastify/cors": "^11.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/query-sync-storage-persister": "^5.74.4",
    "@tanstack/react-query": "^5.67.3",
    "@tanstack/react-query-devtools": "^5.74.6",
    "@tanstack/react-query-persist-client": "^5.74.4",
    "@tanstack/react-router": "^1.112.13",
    "@tanstack/react-virtual": "^3.13.4",
    "@tanstack/zod-adapter": "^1.114.27",
    "@theleop/push-receiver": "^4.4.0",
    "@types/auto-launch": "^5.0.5",
    "auto-launch": "^5.0.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fastify": "^5.3.2",
    "googleapis": "^146.0.0",
    "jotai": "^2.12.2",
    "linkify-react": "^4.2.0",
    "linkifyjs": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.488.0",
    "mime": "^4.0.7",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.0.14",
    "tw-animate-css": "^1.2.5",
    "use-debounce": "^10.0.4",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tanstack/react-router-devtools": "^1.114.25",
    "@tanstack/router-plugin": "^1.114.25",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.13.4",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  }
}
